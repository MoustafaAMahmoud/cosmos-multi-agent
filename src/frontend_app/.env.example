# Frontend Environment Configuration
# Copy this file to .env and update the values as needed

# API Configuration
REACT_APP_API_PROTOCOL=http
REACT_APP_API_HOST=localhost
REACT_APP_API_PORT=8000
REACT_APP_API_BASE_PATH=/api/v1

# Application Branding
REACT_APP_APP_NAME=Deep Research Agent
REACT_APP_APP_SUBTITLE=AI-Powered Comprehensive Research Platform
REACT_APP_COMPANY_NAME=Microsoft Research
REACT_APP_LOGO_PATH=/research-agent-logo.png
REACT_APP_FAVICON_PATH=/favicon.ico

# Research Agent Configuration
REACT_APP_RESEARCH_AGENT_NAME=Research Assistant
REACT_APP_MAX_SOURCES_DISPLAY=50
REACT_APP_DEFAULT_RESEARCH_MODE=exhaustive
REACT_APP_ENABLE_DEBATE_DETAILS=true

# Documentation and Support
REACT_APP_DOCS_URL=https://docs.microsoft.com/azure/ai/research
REACT_APP_SUPPORT_EMAIL=research-support@microsoft.com
REACT_APP_GITHUB_REPO=https://github.com/microsoft/deep-research-agent

# Theme Configuration
REACT_APP_PRIMARY_COLOR=#2E8B57
REACT_APP_SECONDARY_COLOR=#20B2AA
REACT_APP_ACCENT_COLOR=#FF6B6B
REACT_APP_BACKGROUND_LIGHT=#FAFAFA
REACT_APP_BACKGROUND_DARK=#121212

# Feature Flags
REACT_APP_ENABLE_DARK_MODE=true
REACT_APP_ENABLE_SOURCE_PREVIEW=true
REACT_APP_ENABLE_CITATION_EXPORT=true
REACT_APP_ENABLE_RESEARCH_HISTORY=true

# Analytics and Monitoring (Optional)
REACT_APP_ANALYTICS_ID=
REACT_APP_TELEMETRY_ENDPOINT=